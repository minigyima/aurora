{
    "name": "minigyima/aurora",
    "description": "An application framework / starter kit for Laravel that simplifies API development",
    "type": "library",
    "version": "0.1.1",
    "require": {
        "illuminate/support": ">=10.0",
        "illuminate/cache": ">=10.0",
        "illuminate/database": ">=10.0",
        "illuminate/console": ">=10.0",
        "spatie/laravel-ignition": ">=1.0",
        "php": "^8.3",
        "laravel/octane": "^2.2",
        "spatie/file-system-watcher": "^1.1",
        "laravel/prompts": ">=0.1.13",
        "nette/php-generator": "^4.1",
        "laravel/sanctum": "^3.3",
        "symfony/yaml": "^7.0",
        "laravel/horizon": "^5.21",
        "ext-pcntl": "*",
        "ext-posix": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Minigyima\\Aurora\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Máté Gyimesi",
            "email": "gyim.mate@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Minigyima\\Aurora\\Providers\\AuroraServiceProvider"
            ],
            "aliases": {
                "Aurora": "Minigyima\\Aurora\\Facades\\Aurora"
            }
        }
    },
    "minimum-stability": "stable"
}
